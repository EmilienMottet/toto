{
    "version": 2,
    "builds": [
        {
            "src": "static-generator.py",
            "use": "@vercel/python"
        },
        {
            "src": "build/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/main.py"
        }
    ]
}

