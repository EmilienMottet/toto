{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api.py"
        }
    ],
    "env": {
        "TARGET_NAME": "ensimag",
        "DISPLAY_NAME": "Ensimag"
    }
}
